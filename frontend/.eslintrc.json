{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "jsx-a11y/label-has-for": [ 2, {
            "components": [ "Label" ],
            "required": {
                "some": [ "nesting", "id" ]
            },
            "allowChildren": false
        }],
        "react/forbid-prop-types": [false],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}